{
  "name": "fretboard",
  "version": "0.0.1",
  "description": "Programatically handle the notes of fretted string instruments, like the guitar",
  "main": "lib/index.js",
  "scripts": {
    "test": "xo && mocha test"
  },
  "author": "Robert Pirtle",
  "license": "MIT",
  "xo": {
    "esnext": true,
    "space": true,
    "overrides": [
      {
