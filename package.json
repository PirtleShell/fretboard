  },
  "dependencies": {
    "chai": "^3.5.0",
    "xo": "^0.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/PirtleShell/fretboard.git"
  },
  "keywords": [
    "instrument",
    "tuning",
    "music",
    "frets",
    "tune",
    "guitar",
    "stringed",
    "instruments"
